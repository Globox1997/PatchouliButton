{
  "schemaVersion": 1,
  "id": "patchoulibutton",
  "version": "${version}",
  "name": "PachouliButton",
  "description": "This is an addon for the mod Patchouli which adds a button in the inventory screen",
  "authors": [
    "Globox_Z"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/patchoulibutton",
    "sources": "https://github.com/Globox1997/PatchouliButton",
    "issues": "https://github.com/Globox1997/PatchouliButton/issues"
  },
  "license": "MIT",
  "icon": "assets/patchoulibutton/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.patchoulibutton.PatchouliButtonMain"
    ],
    "client": [
      "net.patchoulibutton.PatchouliButtonClient"
    ],
    "modmenu": [
      "net.patchoulibutton.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "patchoulibutton.mixins.json"
  ],
  "depends": {
    "java": ">=17",
    "minecraft": ">=${minecraft_version}",
    "fabric-api": ">=${fabric_version}",
    "fabricloader": ">=${loader_version}",
    "cloth-config2": ">=${cloth_config_version}",
    "patchouli": "*"
  },
  "suggests": {
    "another-mod": "*"
  },
  "custom": {
    "mc-publish": {
      "modrinth": "bTOwvKv9",
      "curseforge": 703778
    }
  }
}